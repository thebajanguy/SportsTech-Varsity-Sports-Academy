{
  "trailingSlash": "auto",
  "mimeTypes": {
    ".json": "text/json"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/images/*",
      "/api/*",
      "/sitemap.xml",
      "/robots.txt",
      "/*.{css,js,png,jpg,jpeg,gif,svg,webp,ico,woff,woff2,ttf,map}"
    ]
  },
  "routes": [
    { "route": "/", "redirect": "/en/academic-tutoring-and-sports-training", "statusCode": 301 },
    { "route": "/index.html", "redirect": "/en/academic-tutoring-and-sports-training", "statusCode": 301 },

    { "route": "/lander", "redirect": "/en/academic-tutoring-and-sports-training", "statusCode": 301 },
    { "route": "/lander/", "redirect": "/en/academic-tutoring-and-sports-training", "statusCode": 301 },

    { "route": "/en/academic-tutoring-and-sports-training/", "redirect": "/en/academic-tutoring-and-sports-training", "statusCode": 301 },
    { "route": "/en/who-we-are/", "redirect": "/en/who-we-are", "statusCode": 301 },
    { "route": "/en/contact/", "redirect": "/en/contact", "statusCode": 301 },
    { "route": "/en/consultation/", "redirect": "/en/consultation", "statusCode": 301 },
    { "route": "/en/what-we-do/academic-tutoring/", "redirect": "/en/what-we-do/academic-tutoring", "statusCode": 301 },
    { "route": "/en/what-we-do/ncaa-compliance/", "redirect": "/en/what-we-do/ncaa-compliance", "statusCode": 301 },
    { "route": "/en/what-we-do/sports-development/", "redirect": "/en/what-we-do/sports-development", "statusCode": 301 },
    { "route": "/en/what-we-do/recruitment-exposure/", "redirect": "/en/what-we-do/recruitment-exposure", "statusCode": 301 },

    { "route": "/en/academic-tutoring-and-sports-training", "rewrite": "/index.html" },
    { "route": "/en/who-we-are", "rewrite": "/index.html" },
    { "route": "/en/contact", "rewrite": "/index.html" },
    { "route": "/en/consultation", "rewrite": "/index.html" },
    { "route": "/en/what-we-do/academic-tutoring", "rewrite": "/index.html" },
    { "route": "/en/what-we-do/ncaa-compliance", "rewrite": "/index.html" },
    { "route": "/en/what-we-do/sports-development", "rewrite": "/index.html" },
    { "route": "/en/what-we-do/recruitment-exposure", "rewrite": "/index.html" }

    { "route": "/sitemap.xml", "serve": "/sitemap.xml" },
    { "route": "/robots.txt", "serve": "/robots.txt" }
  ],
  "globalHeaders": {
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload",
    "X-Frame-Options": "SAMEORIGIN",
    "X-Content-Type-Options": "nosniff"
  },
  "platform": {
    "apiRuntime": "dotnet-isolated:8.0"
  }
}
